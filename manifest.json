{
   "action": {
      "default_icon": {
         "16": "icons/icon16.png",
         "48": "icons/icon48.png",
         "128": "icons/icon128.png"
      },
      "default_popup": "src/popup.html"
   },
   "background": {
      "service_worker": "src/background.js"
   },
   "content_scripts": [
      {
         "js": [
            "src/page-inject.js"
         ],
         "matches": [
            "https://chat.openai.com/*",
            "https://chatgpt.com/*"
         ],
         "run_at": "document_start"
      },
      {
         "js": [
            "src/page-script.js"
         ],
         "matches": [
            "https://chat.openai.com/*",
            "https://chatgpt.com/*"
         ],
         "run_at": "document_start",
         "world": "MAIN"
      },
      {
         "js": [
            "src/constants.js",
            "src/active_badge.js",
            "src/navigation.js",
            "src/boot.js"
         ],
         "matches": [
            "https://chat.openai.com/*",
            "https://chatgpt.com/*"
         ],
         "run_at": "document_idle"
      }
   ],
   "description": "ChatGPT Speed is a Chrome extension that reduces lag in long conversations by trimming what the browser renders.",
   "host_permissions": [
      "https://chat.openai.com/*",
      "https://chatgpt.com/*"
   ],
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
   "manifest_version": 3,
   "name": "ChatGPT Speed - Make long ChatGPT threads fast again.",
   "permissions": [
      "storage",
      "activeTab"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.3",
   "web_accessible_resources": []
}
